@model List<NoteModel>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Title</th>
            <th>Note</th>
        </tr>
    </thead>
    <tbody>
        <!-- Display contact details in a table row -->
        @foreach (var note in Model)
        {
            <tr>
                <td width="25%">@note.Title</td>
                <td width="25%">@note.Note</td>
            </tr>
        }
    </tbody>
</table>

